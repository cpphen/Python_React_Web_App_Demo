{	
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"parser": "babel-eslint",
	"parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
        "jsx": true
    }
  },
	"rules": {
		"no-console": 0,
    "no-unused-vars": 1,
    "semi": 1,
    "strict": 0
	},
	"settings": {
		"react": {
			"version": "16.6.0"
		}
	}
}
